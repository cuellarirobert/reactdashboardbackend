// basque
// hungarian
// estonian
// catalan
// croatian
// romanian


// Export the function to get translations
function getTranslationsForLanguage(languageCode) {
    const translations = { en, es, fr, de, it, ar, nl, sv, da, zh, ru, pt, iw, he, ga, mi, pl, ms, hi, el, cy, tr, fi, sr, no, cs, sq, th}; // Include other language objects here
    return translations[languageCode] || translations.en; // Default to English
}

export default getTranslationsForLanguage;

